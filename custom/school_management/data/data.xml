<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <!-- Sequences for school student -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">school.student</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for school class -->
        <record id="seq_school_class" model="ir.sequence">
            <field name="name">Class Sequence</field>
            <field name="code">school.class</field>
            <field name="prefix">CL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>